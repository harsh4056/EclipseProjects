            <activity
            android:name=".cnv"
            android:label="@string/app_name" >
            </activity>
              <activity
            android:name=".Entry"
            android:label="@string/app_name" >
            </activity>
            <activity
            android:name=".Main"
            android:label="@string/app_name" >
            </activity>
              <activity
            android:name=".AttendanceActivity"
            android:label="@string/app_name" >
            </activity>
            
                 <activity
